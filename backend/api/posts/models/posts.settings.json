{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Posts"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "user": {
      "via": "posts",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
